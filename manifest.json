{
    "name": "GPTLLE",
    "version":"1.0.0",
    "description":"GPT Extention",
    "manifest_version":3,
    "author":"osrbck",
    "action":{
        "default_popup":"index.html"
    },

    "content_scripts":[
        {
            "matches": ["https://mail.google.com/*"],
            "js":["gmail-script.js"]
        }
    ],
    "host_permissions":[
        "https://mail.google.com/*"
    ]
}